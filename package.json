{
  "name": "autorag-clean",
  "version": "1.0.0",
  "description": "AutoRAG Clean - Open-source framework for RAG-powered chatbots",
  "private": true,
  "scripts": {
    "test": "./scripts/test-all.sh",
    "test:coverage": "./scripts/test-coverage.sh",
    "test:worker": "cd worker && npm test",
    "test:widget": "cd widget && npm test",
    "test:worker:watch": "cd worker && npm run test:watch",
    "test:widget:watch": "cd widget && npm run test:watch",
    "test:worker:coverage": "cd worker && npm run test:coverage",
    "test:widget:coverage": "cd widget && npm run test:coverage",
    "install:all": "cd worker && npm install && cd ../widget && npm install",
    "build:widget": "cd widget && npm run build",
    "dev:worker": "cd worker && npm run dev",
    "dev:widget": "cd widget && npm run dev",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "format": "prettier --write \"**/*.{js,ts,json,css,html,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,json,css,html,md}\"",
    "lint:worker": "cd worker && npm run lint",
    "lint:widget": "cd widget && npm run lint",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arnemoor/rag-chatbot.git"
  },
  "keywords": [
    "rag",
    "chatbot",
    "ai",
    "cloudflare-workers",
    "web-components",
    "framework"
  ],
  "author": "AutoRAG Contributors",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "workspaces": [
    "worker",
    "widget"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vitest": "^0.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2"
  }
}
