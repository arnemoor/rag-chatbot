{
  "name": "autorag-worker",
  "version": "0.1.0",
  "description": "AutoRAG Worker with Multi-model Support",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "tail": "wrangler tail --format pretty",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,ts,json}\"",
    "format:check": "prettier --check \"**/*.{js,ts,json}\"",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --reporter=default"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.8.66",
    "@cloudflare/workers-types": "^4.20240117.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "miniflare": "^4.20250816.1",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4",
    "wrangler": "^4.32.0"
  },
  "dependencies": {
    "openai": "^4.26.0"
  }
}
